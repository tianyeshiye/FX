package ty.fx.macd;

import java.util.ArrayList;
import java.util.List;

import ty.fx.utils.ExcelFileUtils;

public class JP225 {

	public static void main(String[] args) throws Exception {

		JP225 JP225 = new JP225();

		JP225.test();
	}

	public void test() {

		Macd instance = new Macd();

		List<String> logList;
		List<String> printList;
		List<String> printSortList;
		List<MacdDataBean> macdList;
		List<MacdResultBean> resulList;

		// instance.calculateMacd(macdList, 30, 65, 23);
		// **********
		// ********** JP225H1-12-h1
		System.out.println("JP225H1-12-h1");
		logList = new ArrayList<String>();
		printList = new ArrayList<String>();
		printSortList = new ArrayList<String>();
		macdList = new ArrayList<MacdDataBean>();
		resulList = new ArrayList<MacdResultBean>();

		macdList = Macds.getInputData("data/JP225H1.csv");
		instance.calculateMacd(macdList, 12, 26, 9);

		logList = instance.ea(macdList, resulList);
		printList = instance.printResult(resulList);
		printSortList = instance.printSortResult(resulList);

		ExcelFileUtils.writeExcel(printList, "output/JP225.xlsx", "JP225H1-12");
		ExcelFileUtils.writeExcel(printSortList, "output/JP225.xlsx", "JP225H1-sort-12");
		ExcelFileUtils.writeExcel(logList, "output/JP225.xlsx", "JP225H1-log-12");

		// **********
		// ********** JP225H4-12-h4
		System.out.println("JP225H1-12-h4");
		logList = new ArrayList<String>();
